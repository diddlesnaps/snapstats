{
  "name": "snapstats",
  "description": "snapstats",
  "version": "0.0.1",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "run-p dev:typescript dev:sapper",
    "dev:sapper": "sapper dev",
    "dev:typescript": "tsc --watch",
    "build": "run-s build:typescript build:sapper build:exports build:packagejson build:npminstall",
    "build:exports": "shx sed -i 's/(require.*)/module.exports = $1/' __sapper__/build/index.js",
    "build:sapper": "sapper build --legacy",
    "build:packagejson": "shx cp package*.json __sapper__/build/",
    "build:npminstall": "npm install --production --prefix __sapper__/build",
    "build:typescript": "tsc",
    "export": "sapper export --legacy",
    "start": "run-s copy-runtimeconfig firebase:emulators",
    "copy-runtimeconfig": "shx cp .runtimeconfig.json __sapper__/build/",
    "firebase:dev": "run-p dev firebase:emulators",
    "firebase:emulators": "firebase emulators:start",
    "deploy": "firebase deploy",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:cypress": "run-p --race dev cy:run",
    "test:mocha": "mocha --reporter spec",
    "test": "npm run test:mocha",
    "validate": "svelte-check"
  },
  "dependencies": {
    "@apollo/client": "^3.4.11",
    "@beyonk/gdpr-cookie-consent-banner": "^7.1.1",
    "@google-cloud/pubsub": "^2.17.0",
    "@graphql-tools/merge": "^8.1.2",
    "@graphql-tools/schema": "^8.2.0",
    "@grpc/grpc-js": "^1.3.2",
    "apollo-server-cloud-functions": "^3.3.0",
    "chart.js": "^3.5.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "color-rainbow": "0.0.0",
    "date-fns": "^2.23.0",
    "dompurify": "^2.3.1",
    "fb": "^2.0.0",
    "feed": "^4.2.2",
    "firebase": "^9.0.2",
    "firebase-admin": "^9.11.1",
    "firebase-functions": "^3.15.5",
    "fslightbox": "^3.3.0-2",
    "graphql": "^15.5.3",
    "graphql-dataloader-mongoose": "^1.0.1",
    "ignore-styles": "^5.0.1",
    "js-yaml": "^4.1.0",
    "jsdom": "^17.0.0",
    "lodash.escaperegexp": "^4.1.2",
    "marked": "^3.0.3",
    "mongoose": "^6.0.5",
    "node-fetch": "^2.6.2",
    "paginator": "^1.0.0",
    "random-js": "^2.1.0",
    "sitemap": "^7.0.0",
    "twitter-lite": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/runtime": "^7.15.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tsconfig/svelte": "^2.0.1",
    "@types/chart.js": "^2.9.34",
    "@types/dompurify": "^2.2.3",
    "@types/express": "^4.17.13",
    "@types/lodash.escaperegexp": "^4.1.6",
    "@types/marked": "^3.0.1",
    "@types/node-fetch": "^2.5.12",
    "compression": "^1.7.4",
    "cssnano": "^5.0.8",
    "cssnano-preset-advanced": "^5.1.4",
    "express": "^4.17.1",
    "firebase-functions-test": "^0.3.2",
    "firebase-tools": "^9.18.0",
    "mocha": "^9.1.1",
    "npm-run-all": "^4.1.5",
    "postcss-load-config": "^3.1.0",
    "rollup": "^2.56.3",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.2",
    "shx": "^0.3.3",
    "sirv": "^1.0.17",
    "svelte": "^3.42.5",
    "svelte-apollo": "^0.4.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}

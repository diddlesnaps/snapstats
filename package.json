{
  "name": "snapstats",
  "description": "snapstats",
  "version": "0.0.1",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "dev": "run-p dev:typescript dev:sapper",
    "dev:sapper": "sapper dev",
    "dev:typescript": "tsc --watch",
    "build": "run-s build:sapper build:exports build:packagejson build:npminstall",
    "build:exports": "shx sed -i 's/(require.*)/module.exports = $1/' __sapper__/build/index.js",
    "build:sapper": "sapper build --legacy",
    "build:packagejson": "shx cp package*.json __sapper__/build/",
    "build:npminstall": "npm install --production --prefix __sapper__/build",
    "build:typescript": "tsc",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "firebase:dev": "run-p dev firebase:emulators",
    "firebase:emulators": "firebase emulators:start",
    "deploy": "firebase deploy",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:cypress": "run-p --race dev cy:run",
    "test:mocha": "mocha --reporter spec",
    "test": "npm run test:mocha",
    "validate": "svelte-check"
  },
  "dependencies": {
    "@beyonk/gdpr-cookie-consent-banner": "^7.0.1",
    "@google-cloud/pubsub": "^2.9.0",
    "@graphql-tools/merge": "^6.2.7",
    "@grpc/grpc-js": "^0.6.18",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-server-cloud-functions": "^2.21.0",
    "apollo-server-express": "^2.21.0",
    "chart.js": "^2.9.4",
    "color-rainbow": "0.0.0",
    "dompurify": "^2.2.6",
    "fb": "^2.0.0",
    "feed": "^4.2.2",
    "firebase-admin": "^9.5.0",
    "firebase-functions": "^3.13.1",
    "fslightbox": "^3.2.3",
    "graphql": "^14.7.0",
    "graphql-dataloader-mongoose": "^1.0.1",
    "graphql-tag": "^2.11.0",
    "graphql-tag-pluck": "^0.8.7",
    "graphql-tools": "^5.0.0",
    "ignore-styles": "^5.0.1",
    "js-yaml": "^3.14.1",
    "jsdom": "^16.4.0",
    "lodash.escaperegexp": "^4.1.2",
    "marked": "^2.0.0",
    "mongoose": "^5.11.16",
    "node-fetch": "^2.6.1",
    "paginator": "^1.0.0",
    "random-js": "^2.1.0",
    "sitemap": "^6.3.6",
    "subscriptions-transport-ws": "^0.9.18",
    "svelte-stars-rating": "^1.0.3",
    "twitter-lite": "^0.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.16",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.16",
    "@babel/runtime": "^7.12.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "@types/chart.js": "^2.9.30",
    "@types/express": "^4.17.11",
    "@types/lodash.escaperegexp": "^4.1.6",
    "@types/mongoose": "^5.10.3",
    "@types/node-fetch": "^2.5.8",
    "compression": "^1.7.4",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "express": "^4.17.1",
    "firebase-functions-test": "^0.2.3",
    "firebase-tools": "^9.3.0",
    "mocha": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-load-config": "^2.1.2",
    "rollup": "^2.39.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.1",
    "shx": "^0.3.3",
    "sirv": "^1.0.11",

{
  "name": "snapstats",
  "description": "snapstats",
  "version": "0.0.1",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "dev": "run-p dev:typescript dev:sapper",
    "dev:sapper": "sapper dev",
    "dev:typescript": "tsc --watch",
    "build": "run-s build:sapper build:exports build:packagejson build:npminstall",
    "build:exports": "sed -i -E 's/(require.*)/module.exports = \\1/' __sapper__/build/index.js",
    "build:sapper": "sapper build --legacy",
    "build:packagejson": "cp package*.json __sapper__/build/",
    "build:npminstall": "npm install --production --prefix __sapper__/build",
    "build:typescript": "tsc",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "firebase:dev": "run-p dev firebase:emulators",
    "firebase:emulators": "firebase emulators:start",
    "deploy": "firebase deploy",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:cypress": "run-p --race dev cy:run",
    "test:mocha": "mocha --reporter spec",
    "test": "npm run test:mocha",
    "validate": "svelte-check"
  },
  "dependencies": {
    "@beyonk/gdpr-cookie-consent-banner": "^7.0.1",
    "@firebase/app": "^0.4.26",
    "@google-cloud/pubsub": "^1.7.3",
    "@grpc/grpc-js": "^0.6.18",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-server-cloud-functions": "^2.17.0",
    "apollo-server-express": "^2.17.0",
    "chart.js": "^2.9.3",
    "color-rainbow": "0.0.0",
    "compression": "^1.7.4",
    "dompurify": "^2.0.15",
    "express": "^4.17.1",
    "fb": "^2.0.0",
    "feed": "^4.2.1",
    "firebase": "^7.19.1",
    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.11.0",
    "fslightbox": "^3.2.1",
    "graphql": "^14.7.0",
    "graphql-dataloader-mongoose": "^1.0.1",
    "graphql-tag": "^2.11.0",
    "graphql-tag-pluck": "^0.8.7",
    "graphql-tools": "^4.0.8",
    "ignore-styles": "^5.0.1",
    "js-yaml": "^3.14.0",
    "jsdom": "^16.4.0",
    "lodash.escaperegexp": "^4.1.2",
    "marked": "^0.7.0",
    "mongoose": "^5.10.3",
    "node-fetch": "^2.6.0",
    "paginator": "^1.0.0",
    "random-js": "^2.1.0",
    "sirv": "^0.4.6",
    "sitemap": "^4.1.1",
    "subscriptions-transport-ws": "^0.9.18",
    "svelte-stars-rating": "^1.0.3",
    "twitter-lite": "^0.9.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@graphql-tools/merge": "^6.2.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "@types/chart.js": "^2.9.24",
    "@types/express": "^4.17.8",
    "@types/lodash.escaperegexp": "^4.1.6",
    "@types/mongoose": "^5.7.36",
    "@types/node-fetch": "^2.5.7",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "firebase-functions-test": "^0.2.2",
    "firebase-tools": "^8.9.2",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-load-config": "^2.1.0",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^7.0.1",
    "sapper": "^0.27.16",

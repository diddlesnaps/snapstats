{
  "name": "snapstats",
  "description": "snapstats",
  "version": "0.0.1",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "dev": "run-p dev:typescript dev:sapper",
    "dev:sapper": "sapper dev",
    "dev:typescript": "tsc --watch",
    "build": "run-s build:typescript build:sapper build:exports build:packagejson build:npminstall",
    "build:exports": "shx sed -i 's/(require.*)/module.exports = $1/' __sapper__/build/index.js",
    "build:sapper": "sapper build --legacy",
    "build:packagejson": "shx cp package*.json __sapper__/build/",
    "build:npminstall": "npm install --production --prefix __sapper__/build",
    "build:typescript": "tsc",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "firebase:dev": "run-p dev firebase:emulators",
    "firebase:emulators": "firebase emulators:start",
    "deploy": "firebase deploy",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:cypress": "run-p --race dev cy:run",
    "test:mocha": "mocha --reporter spec",
    "test": "npm run test:mocha",
    "validate": "svelte-check"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@beyonk/gdpr-cookie-consent-banner": "^7.1.1",
    "@google-cloud/pubsub": "^2.12.0",
    "@graphql-tools/merge": "^6.2.14",
    "@graphql-tools/schema": "^7.1.5",
    "@grpc/grpc-js": "^1.3.2",
    "apollo-server-cloud-functions": "^2.25.0",
    "apollo-server-express": "^2.25.0",
    "chart.js": "^2.9.4",
    "color-rainbow": "0.0.0",
    "dompurify": "^2.2.8",
    "fb": "^2.0.0",
    "feed": "^4.2.2",
    "firebase-admin": "^9.9.0",
    "firebase-functions": "^3.14.1",
    "fslightbox": "^3.2.3",
    "graphql": "^15.5.0",
    "graphql-dataloader-mongoose": "^1.0.1",
    "ignore-styles": "^5.0.1",
    "js-yaml": "^4.1.0",
    "jsdom": "^16.6.0",
    "lodash.escaperegexp": "^4.1.2",
    "marked": "^2.0.6",
    "mongoose": "^5.12.12",
    "node-fetch": "^2.6.1",
    "paginator": "^1.0.0",
    "random-js": "^2.1.0",
    "sitemap": "^6.3.6",
    "twitter-lite": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.2",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/runtime": "^7.14.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.13",
    "@types/chart.js": "^2.9.32",
    "@types/express": "^4.17.12",
    "@types/lodash.escaperegexp": "^4.1.6",
    "@types/node-fetch": "^2.5.10",
    "compression": "^1.7.4",
    "cssnano": "^5.0.6",
    "cssnano-preset-advanced": "^5.1.3",
    "express": "^4.17.1",
    "firebase-functions-test": "^0.2.3",
    "firebase-tools": "^9.16.6",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "postcss-load-config": "^2.1.2",
    "rollup": "^2.50.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.1",
    "shx": "^0.3.3",
    "sirv": "^1.0.12",
    "svelte": "^3.38.2",
    "svelte-apollo": "^0.4.0",
    "svelte-check": "^1.6.0",
    "svelte-preprocess": "^4.7.3",
    "tslib": "^2.1.0",
    "typescript": "^4.3.2"
  }
}
